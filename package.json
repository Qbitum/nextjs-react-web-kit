{
  "name": "qb-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel next-dev",
    "next-dev": "next dev -p 4000",
    "build": "next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "test": "jest",
    "coverage": "jest --coverage",
    "prepare": "husky install",
    "pre-commit": "next lint",
    "restify": "node ./restify.setup.js",
    "generateapis": "rm -rf ./src/services; openapi-generator-cli generate -i ./template_apis.yaml -o ./src/services -g typescript-axios --additional-properties=supportsES6=true,npmVersion=8.5.0,typescriptThreePlus=true,modelPackage=model",
    "commit": "cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ./husky/commit-msg/HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.9",
    "@types/node": "20.6.0",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "axios": "^1.6.5",
    "classnames": "^2.5.1",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "formik": "^2.4.5",
    "framer-motion": "^10.17.6",
    "i18next": "^23.11.3",
    "i18next-browser-languagedetector": "^7.2.1",
    "i18next-http-backend": "^2.5.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^23.0.4",
    "lint": "^0.8.19",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "pkce-challenge": "^4.0.1",
    "react": "18.2.0",
    "react-abac": "^0.1.17",
    "react-dom": "18.2.0",
    "react-helmet": "^6.1.0",
    "react-html5-camera-photo": "^1.5.11",
    "react-i18next": "^14.1.1",
    "react-icons": "^4.12.0",
    "react-oauth2-code-pkce": "^1.16.0",
    "react-select": "^5.8.0",
    "react-toastify": "^9.1.3",
    "react-use": "^17.5.0",
    "react-uuid": "^2.0.0",
    "react-webcam": "^7.2.0",
    "typescript": "5.2.2",
    "uuid": "^9.0.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@commitlint/cz-commitlint": "^18.4.3",
    "@commitlint/prompt-cli": "^18.4.3",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.11",
    "@types/js-cookie": "^3.0.6",
    "@types/react-helmet": "^6.1.11",
    "@types/react-html5-camera-photo": "^1.5.3",
    "@types/uuid": "^9.0.7",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "restapify": "^2.3.4",
    "tailwindcss": "^3.4.3",
    "webpack-node-externals": "^3.0.0"
  }
}
